@model IEnumerable<MvcBlogSitesi.Models.Makale>
@{
    Layout = null;
}
<div class="card my-4">
    <h5 class="card-header">Popüler Makaleler</h5>
    <div class="card-body">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.MakaleNo">@item.MakaleBaşlık </a>(@item.MakaleOkunma)</li>
            }
        </ul>
    </div>
</div>
